{
  "name": "ai-feed-consolidator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "cleanup": "(lsof -ti:3003,5173 | xargs kill -9) || true",
    "dev": "npm run cleanup && concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "vite",
    "dev:server": "tsx watch src/server/index.ts",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "migrate": "node scripts/db.cjs migrate",
    "migrate:status": "node scripts/db.cjs migrate:status",
    "migrate:create": "node scripts/db.cjs migrate:create",
    "migrate:verify": "node scripts/db.cjs verify",
    "migrate:reset": "node scripts/db.cjs reset",
    "db": "node scripts/db.cjs",
    "db:types": "node scripts/db.cjs generate:types",
    "test:db:setup": "NODE_ENV=test node scripts/db.cjs reset && NODE_ENV=test node scripts/db.cjs migrate --no-backup",
    "test": "NODE_OPTIONS='--max-old-space-size=8192' DATABASE_URL=postgresql://postgres:postgres@localhost:5433/ai-feed-test NODE_ENV=test vitest",
    "test:server": "NODE_OPTIONS='--max-old-space-size=8192' DATABASE_URL=postgresql://postgres:postgres@localhost:5433/ai-feed-test NODE_ENV=test vitest run -c vitest.config.ts",
    "test:client": "NODE_OPTIONS='--max-old-space-size=8192' NODE_ENV=test vitest run -c vitest.jsdom.config.ts",
    "test:ci": "npm run test:server && npm run test:client",
    "test:coverage": "DATABASE_URL=postgresql://postgres:postgres@localhost:5433/ai-feed-test NODE_ENV=test vitest run --coverage"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.1",
    "@mui/material": "^6.4.1",
    "@tanstack/react-query": "^5.64.2",
    "@tanstack/react-query-devtools": "^5.65.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-pino-logger": "^4.0.5",
    "@types/express-session": "^1.18.1",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/pino": "^7.0.5",
    "@types/pino-multi-stream": "^5.1.6",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.7.9",
    "axios-retry": "^4.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-pino-logger": "^7.0.0",
    "express-session": "^1.18.1",
    "fast-xml-parser": "^4.5.1",
    "feedparser": "^2.2.10",
    "firebase": "^11.4.0",
    "firebase-admin": "^13.1.0",
    "memorystore": "^1.6.7",
    "multer": "^1.4.5-lts.1",
    "nock": "^14.0.0",
    "openai": "^4.79.4",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg-promise": "^11.10.2",
    "pino": "^9.6.0",
    "pino-multi-stream": "^6.0.0",
    "pino-pretty": "^13.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.2.0",
    "rss-parser": "^3.13.0",
    "xml2js": "^0.6.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.0",
    "@types/jest": "^29.5.14",
    "@types/jsdom": "^21.1.7",
    "@types/multer": "^1.4.12",
    "@types/pg": "^8.11.10",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/supertest": "^6.0.2",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "jsdom": "^26.0.0",
    "kill-port": "^2.0.1",
    "msw": "^2.4.11",
    "pg": "^8.13.1",
    "prettier": "^3.4.2",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vitest": "^3.0.2",
    "web-streams-polyfill": "^4.1.0"
  }
}
